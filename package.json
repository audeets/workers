{
  "name": "audeets-services",
  "version": "1.0.0",
  "type": "module",
  "private": true,
  "author": "Benoit Quette",
  "scripts": {
    "test": "npm run coverage && npm run check-coverage",
    "coverage": "istanbul cover node_modules/mocha/bin/_mocha -- --recursive \"./test/unit/**/*Spec.js\"",
    "check-coverage": "istanbul check-coverage --statement 85",
    "start:gpsi": "node --env-file=.env ./bin/services.js start audit-performance-gpsi",
    "start:seobasic": "node --env-file=.env ./bin/services.js start audit-search-basic",
    "start:mongo": "node --env-file=.env ./bin/services.js start store-mongo",
    "start:elastic": "node --env-file=.env ./bin/services.js start store-elasticsearch",
    "start:crawl": "node --env-file=.env ./bin/services.js start crawler",
    "audit": "node --env-file=.env ./bin/services.js audit"
  },
  "dependencies": {
    "@benoitquette/audeets-api-commons": "^1.0.7",
    "ajv": "^4.1.3",
    "amqplib": "^0.4.1",
    "arrive": "^2.3.1",
    "async": "^2.0.1",
    "cheerio": "^0.20.0",
    "colors": "^1.1.2",
    "commander": "^2.9.0",
    "cookie-parser": "~1.4.3",
    "debug": "~2.2.0",
    "ejs": "^2.5.2",
    "jquery": "^3.0.0",
    "lodash": "^4.16.4",
    "moment": "^2.15.1",
    "morgan": "~1.7.0",
    "node-phantom-simple": "^2.2.4",
    "os": "^0.1.1",
    "phantom": "^2.1.10",
    "rabbitmq-schema": "^1.1.1",
    "recompose": "^0.20.2",
    "traverse": "^0.6.6",
    "winston": "^2.2.0",
    "winston-config": "^0.4.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.23.4",
    "@babel/core": "^7.23.7",
    "@babel/eslint-parser": "^7.23.3",
    "@babel/preset-env": "^7.23.8",
    "chai": "^3.5.0",
    "chai-things": "^0.2.0",
    "connect": "^3.4.1",
    "cross-env": "^2.0.1",
    "css-loader": "^0.23.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.2",
    "file-loader": "^0.9.0",
    "finalhandler": "^0.5.0",
    "istanbul": "^0.4.3",
    "mocha": "^2.5.3",
    "ngrok": "^2.2.1",
    "proxyquire": "^1.7.10",
    "serve-static": "^1.11.1",
    "sinon": "^1.17.4",
    "style-loader": "^0.13.1"
  },
  "babel": {
    "presets": [
      [
        "@babel/env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ],
    "plugins": [
      "@babel/plugin-transform-class-properties",
      "@babel/plugin-transform-object-rest-spread"
    ]
  }
}