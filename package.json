{
  "name": "audeets-services",
  "version": "1.0.0",
  "type": "module",
  "private": true,
  "author": "Benoit Quette",
  "scripts": {
    "test": "npm run coverage && npm run check-coverage",
    "coverage": "istanbul cover node_modules/mocha/bin/_mocha -- --recursive \"./test/**/*Spec.js\"",
    "check-coverage": "istanbul check-coverage --statement 85",
    "start:gpsi": "node --env-file=.env ./bin/services.js start audit-performance-gpsi",
    "start:seobasic": "node --env-file=.env ./bin/services.js start audit-search-basic",
    "start:accessbasic": "node --env-file=.env ./bin/services.js start audit-accessibility-basic",
    "start:securitybasic": "node --env-file=.env ./bin/services.js start audit-security-basic",
    "start:mongo": "node --env-file=.env ./bin/services.js start store-mongo",
    "start:elastic": "node --env-file=.env ./bin/services.js start store-elasticsearch",
    "start:crawl": "node --env-file=.env ./bin/services.js start crawler",
    "audit": "node --env-file=.env ./bin/services.js audit"
  },
  "dependencies": {
    "@benoitquette/audeets-api-commons": "^1.0.14",
    "ajv": "^8.12.0",
    "amqplib": "^0.10.3",
    "arrive": "^2.4.1",
    "async": "^3.2.5",
    "chai-like": "^1.1.1",
    "cheerio": "^0.22.0",
    "colors": "^1.4.0",
    "commander": "^12.0.0",
    "cookie-parser": "~1.4.6",
    "debug": "~4.3.4",
    "ejs": "^3.1.9",
    "esmock": "^2.6.3",
    "jquery": "^3.7.1",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "morgan": "~1.10.0",
    "node-phantom-simple": "^2.2.4",
    "os": "^0.1.2",
    "phantom": "^2.1.10",
    "rabbitmq-schema": "^2.0.1",
    "recompose": "^0.30.0",
    "traverse": "^0.6.8",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.23.9",
    "@babel/core": "^7.23.9",
    "@babel/eslint-parser": "^7.23.10",
    "@babel/preset-env": "^7.23.9",
    "chai": "4.4.1",
    "chai-things": "^0.2.0",
    "connect": "^3.7.0",
    "css-loader": "^6.10.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "file-loader": "^6.2.0",
    "finalhandler": "^1.2.0",
    "istanbul": "^0.4.3",
    "mocha": "^10.3.0",
    "serve-static": "^1.15.0",
    "sinon": "^17.0.1",
    "style-loader": "^3.3.4"
  },
  "babel": {
    "presets": [
      [
        "@babel/env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ],
    "plugins": [
      "@babel/plugin-transform-class-properties",
      "@babel/plugin-transform-object-rest-spread"
    ]
  }
}
